{
  "name": "wecelebrate-app",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:staging": "VITE_APP_ENV=staging vite build",
    "build:production": "VITE_APP_ENV=production vite build",
    "preview": "vite preview",
    "preview:staging": "VITE_APP_ENV=staging vite preview",
    "preview:production": "VITE_APP_ENV=production vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -f vite.config.d.ts vite.config.minimal.d.ts vitest.config.d.ts playwright.config.d.ts *.d.ts 2>/dev/null || true",
    "clean:all": "npm run clean && rm -rf node_modules/.vite node_modules/.tmp/*.tsbuildinfo 2>/dev/null || true",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:baseline": "node scripts/lint-baseline.js",
    "lint:validate": "node scripts/lint-validate.js",
    "lint:validate:verbose": "node scripts/lint-validate.js --verbose",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "echo '⚠️  Use test:safe for local testing or test:full for CI' && exit 1",
    "test:safe": "vitest --run --maxConcurrency=1 --poolOptions.threads.maxThreads=1",
    "test:full": "NODE_OPTIONS='--max-old-space-size=4096' vitest --run --isolate=true --maxConcurrency=6 --poolOptions.threads.maxThreads=6 --poolOptions.threads.singleThread=false",
    "test:kill": "bash scripts/kill-vitest.sh",
    "test:all": "node scripts/test-all.js",
    "test:all:verbose": "node scripts/test-all.js --verbose",
    "test:all:coverage": "node scripts/test-all.js --coverage",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --run --coverage",
    "test:changed": "vitest --run --changed",
    "test:related": "vitest --run --related",
    "test:button": "vitest --run src/app/components/ui/__tests__/button.test.tsx",
    "test:button:watch": "vitest src/app/components/ui/__tests__/button.test.tsx",
    "test:ui-components": "vitest --run src/app/components/ui/__tests__/",
    "test:ui-components:watch": "vitest src/app/components/ui/__tests__/",
    "test:integration": "vitest --run src/app/__tests__/integration/",
    "test:integration:watch": "vitest src/app/__tests__/integration/",
    "test:type-tests": "vitest --run src/app/types/__tests__/ src/types/__tests__/",
    "test:utils": "vitest --run src/app/utils/__tests__/",
    "test:app-components": "vitest --run src/app/components/__tests__/",
    "test:admin-components": "vitest --run src/app/components/admin/__tests__/",
    "test:contexts": "vitest --run src/app/context/__tests__/",
    "test:services": "vitest --run src/app/services/__tests__/ src/services/__tests__/",
    "test:hooks": "vitest --run src/app/hooks/__tests__/",
    "test:pages-user": "vitest --run src/app/pages/__tests__/",
    "test:pages-admin": "vitest --run src/app/pages/admin/__tests__/",
    "test:backend": "vitest --run supabase/functions/server/tests/",
    "test:bulkimport": "vitest --run src/app/utils/__tests__/bulkImport.test.tsx",
    "test:dashboard": "vitest --run src/app/pages/admin/__tests__/Dashboard.test.tsx",
    "test:dashboard-integration": "vitest --run src/app/pages/admin/__tests__/Dashboard.integration.test.tsx",
    "db-optimizer:analyze": "tsx src/db-optimizer/cli.ts analyze",
    "db-optimizer:generate": "tsx src/db-optimizer/cli.ts generate",
    "db-optimizer:analyze-indexes": "tsx src/db-optimizer/cli.ts analyze-indexes",
    "db-optimizer:generate-index-migrations": "tsx src/db-optimizer/cli.ts generate-index-migrations",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "seed:test-data": "tsx src/scripts/seedTestData.ts",
    "seed:test-data:clear": "tsx src/scripts/seedTestData.ts --clear",
    "seed:test-data:simple": "tsx src/scripts/seedTestData.ts --simple",
    "dev": "vite"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
